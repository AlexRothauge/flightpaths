{
  "name": "@flight-paths/frontend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^18.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:coverage": "yarn test:unit -- --coverage",
    "test:unit:ci": "yarn test:unit:coverage -- --run --reporter=junit",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "test:e2e:ci": "cypress run --e2e",
    "build-only": "vite build",
    "type-check": "run-p type-check:app type-check:vitest",
    "type-check:app": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "type-check:vitest": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src cypress --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../../.gitignore",
    "lint:fix": "yarn lint --fix",
    "lint:check": "yarn lint",
    "format": "prettier . '!tsconfig.json' --ignore-path ../../.gitignore",
    "format:write": "yarn format --write",
    "format:check": "yarn format --check",
    "generate:api": "yarn openapi-typescript --output src/types/schema.ts",
    "generate:api:local": "yarn generate:api ../backend/src/api/routes/openapi/swagger.json",
    "generate:api:docker": "yarn generate:api swagger.json"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "@vuepic/vue-datepicker": "^3.5.3",
    "pinia": "^2.0.28",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.10.0",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^6.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-vuetify": "^1.0.1",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  }
}
